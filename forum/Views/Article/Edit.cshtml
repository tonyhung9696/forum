    @section Scripts {
        <script>
            var grid_chooser = "#grid_chooser";
            var grid_content = "";
            for (var i = 0; i < 10; i++) {
                for (var j = 0; j < 10; j++) {
                    grid_content += '<div data-x="' + (i + 1) + '" data-y="' + (j + 1) + '" class="grid_chooser_bit" '
                        + ' onmouseover="dashboard_configurator.on_grid_selector_hover(this)"'
                        + ' onclick="dashboard_configurator.on_grid_selector_click(this)"></div>';
                }
            }
            $(grid_chooser).append(grid_content);

            var draggable = $('.c-topbar'); //element

            draggable.on('mousedown', function (e) {
                var CenterScreen = $(".CenterScreen")
                var dr = $(CenterScreen).addClass("drag").css("cursor", "move");
                height = dr.outerHeight();
                width = dr.outerWidth();
                //max_left = dr.parent().offset().left + dr.parent().width() - dr.width();
                //max_top = dr.parent().offset().top + dr.parent().height() - dr.height();
                max_left = $(window).width() - CenterScreen.width();
                max_top = $(window).height() - CenterScreen.height();
                //min_left = dr.parent().offset().left;
                //min_top = dr.parent().offset().top;
                min_left = 8;
                min_top = 8;
                console.log('max_left' + max_left);
                console.log('max_top' + max_top);
                console.log('min_left' + min_left);
                console.log('min_top' + min_top);

                ypos = dr.offset().top + height - e.pageY,
                    xpos = dr.offset().left + width - e.pageX;
                $(document.body).on('mousemove', function (e) {
                    var itop = e.pageY + ypos - height;
                    var ileft = e.pageX + xpos - width;

                    if (dr.hasClass("drag")) {
                        if (itop <= min_top) { itop = min_top; }
                        if (ileft <= min_left) { ileft = min_left; }
                        if (itop >= max_top) { itop = max_top; }
                        if (ileft >= max_left) { ileft = max_left; }
                        dr.offset({ top: itop, left: ileft });
                    }
                }).on('mouseup', function (e) {
                    dr.removeClass("drag");
                    $(CenterScreen).css("cursor", "default");
                });
            });

        </script>
    }
</form>